@article{chari_sigma2:_2008,
	title = {{SIGMA}2: a system for the integrative genomic multi-dimensional analysis of cancer genomes, epigenomes, and transcriptomes.},
	volume = {9},
	issn = {1471-2105},
	url = {http://www.biomedcentral.com/1471-2105/9/422},
