	keywords = {Astrocytoma, Astrocytoma: genetics, Carcinoma, Carcinoma: genetics, Chromosome Aberrations, Data Interpretation, Gene Dosage, Gene Expression Profiling, Genomics, Genomics: methods, Humans, Kidney Neoplasms, Kidney Neoplasms: genetics, Neoplasms, Neoplasms: genetics, Oligonucleotide Array Sequence Analysis, Polymorphism, Single Nucleotide, Statistical},
	pages = {5057--70}
}

