}

@article{baty_analysis_2006,
	title = {Analysis with respect to instrumental variables for the exploration of microarray data structures.},
	volume = {7},
	issn = {1471-2105},
	url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1594581&tool=pmcentrez&rendertype=abstract},
	doi = {10.1186/1471-2105-7-422},
	abstract = {BACKGROUND: Evaluating the importance of the different sources of variations is essential in microarray data experiments. Complex experimental designs generally include various factors structuring the data which should be taken into account. The objective of these experiments is the exploration of some given factors while controlling other factors. RESULTS: We present here a family of methods, the analyses with respect to instrumental variables, which can be easily applied to the particular case of microarray data. An illustrative example of analysis with instrumental variables is given in the case of microarray data investigating the effect of beverage intake on peripheral blood gene expression. This approach is compared to an ANOVA-based gene-by-gene statistical method. CONCLUSION: Instrumental variables analyses provide a simple way to control several sources of variation in a multivariate analysis of microarray data. Due to their flexibility, these methods can be associated with a large range of ordination techniques combined with one or several qualitative and/or quantitative descriptive variables.},
	journal = {BMC bioinformatics},
	author = {Baty, Florent and Facompré, Michaël and Wiegand, Jan and Schwager, Joseph and Brutsche, Martin H},
	month = jan,
	year = {2006},
