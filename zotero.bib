	keywords = {Biology, Computational Biology, Gastroenterology and Hepatology, Genetics and Genomics, Genome expression analysis, Genomics, gut microbiota metatranscriptomics, Medicine, Metagenomics, Microbial ecology, Microbial physiology, Microbiology, Research Article, Small intestine},
	pages = {e17447}
}

