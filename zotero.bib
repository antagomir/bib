	month = feb,
	year = {2011},
	keywords = {Basic cancer research, Biology, Cancer detection and diagnosis, Cancer genetics, Computational Biology, gene expression, Gene regulation, Genetics, Genetics and Genomics, Genome expression analysis, Genomics, Medicine, Microarrays, Molecular genetics, Oncology, pint-jatko miRNA mRNA expression integration, Research Article},
	pages = {e16915}
