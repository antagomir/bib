@article{wilmes_application_2004,
	title = {The application of two-dimensional polyacrylamide gel electrophoresis and downstream analyses to a mixed community of prokaryotic microorganisms.},
	volume = {6},
	issn = {1462-2912},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/15305916},
	doi = {10.1111/j.1462-2920.2004.00687.x},
	abstract = {Summary In the post-genomic era, the focus of numerous researchers has moved to studying the functional products of gene expression. In microbiology, these "omic" approaches have largely been limited to pure cultures of microorganisms. Consequently, they do not provide information on gene expression in a complex mixture of microorganisms as found in the environment. Our method enabled the successful extraction and purification of the entire proteome from a laboratory-scale activated sludge system optimized for enhanced biological phosphorus removal, its separation by two-dimensional polyacrylamide gel electrophoresis and the mapping of this metaproteome. Highly expressed protein spots were excised and identified using quadrupole time-of-flight mass spectrometry with de novo peptide sequencing. The proteins isolated were putatively identified as an outer membrane protein (porin), an acetyl coenzyme A acetyltransferase and a protein component of an ABC-type branched-chain amino acid transport system. These proteins possibly stem from the dominant and uncultured Rhodocyclus-type polyphosphate-accumulating organism in the activated sludge. We propose the term "metaproteomics" for the large-scale characterization of the entire protein complement of environmental microbiota at a given point in time.},
	number = {9},
	journal = {Environmental microbiology},
	author = {Wilmes, Paul and Bond, Philip L},
	month = sep,
	year = {2004},
	pmid = {15305916},
	keywords = {Acetyl-CoA C-Acyltransferase, Acetyl-CoA C-Acyltransferase: genetics, Amino Acid Sequence, Amino Acid Transport Systems, Amino Acid Transport Systems: genetics, Bacterial Proteins, Bacterial Proteins: genetics, Chromosome Mapping, Electrophoresis, Fluorescence, Gel, gene expression, In Situ Hybridization, Mass, Matrix-Assisted Laser Desorpti, Molecular Sequence Data, Oligonucleotide Probes, Porins, Porins: genetics, Protein, Proteomics, Proteomics: methods, Sequence Analysis, Sewage, Sewage: microbiology, Spectrometry, Two-Dimensional, Two-Dimensional: methods},
	pages = {911--20}
