	keywords = {Animals, Calcium Signaling, cell signaling, Cluster Analysis, cross-talk, crosstalk, Cyclic AMP, Cyclic AMP: biosynthesis, Cytokines, Cytokines: biosynthesis, Ligands, Macrophages, Mice, pathway, Phosphorylation, Receptor Cross-Talk, Signal Transduction},
	pages = {571--80}
}

@article{natrajan_integrative_2010,
