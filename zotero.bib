@article{ma_bayesian_2011,
	title = {Bayesian {Estimation} of {Beta} {Mixture} {Models} with {Variational} {Inference}.},
	volume = {33},
	issn = {1939-3539},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/21422484},
	doi = {10.1109/TPAMI.2011.63},
	abstract = {Bayesian estimation of the parameters in beta mixture models (BMM) is analytically intractable. The numerical solutions to simulate the posterior distribution are available, but incur high computational cost. In this paper, we introduce an approximation to the prior/posterior distribution of the parameters in the beta distribution and propose an analytically tractable (closed-form) Bayesian approach to the parameter estimation. The approach is based on the variational inference (VI) framework. Following the principles of the VI framework and utilizing the relative convexity bound, the extended factorized approximation method is applied to approximate the distribution of the parameters in BMM. In a fully Bayesian model where all the parameters of the BMM are considered as variables and assigned proper distributions, our approach can asymptotically find the optimal estimate of the parameters posterior distribution. Also, the model complexity can be determined based on the data. The closed-form solution is proposed so that no iterative numerical calculation is required. Meanwhile, our approach avoids the drawback of overfitting in the conventional expectation maximization algorithm. The good performance of this approach is verified by experiments with both synthetic and real data.},
	number = {11},
	journal = {IEEE transactions on pattern analysis and machine intelligence},
	author = {Ma, Zhanyu and Leijon, Arne},
	month = mar,
	year = {2011},
	pmid = {21422484},
	keywords = {Bayesian, beta mixture, mixture model, variational},
	pages = {2160--2173}
}

